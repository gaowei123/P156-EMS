<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="EMS.OutputTemp"
	x:Name="UserControl"
	Width="180" Height="53">

		<Grid x:Name="LayoutRoot">
		<Grid Width="180">
			<Rectangle Stroke="{DynamicResource Linear_BlackSilver}" RadiusX="5" RadiusY="5" Fill="{DynamicResource Linear_Silver}"/>
			<Button x:Name="btn" Style="{DynamicResource ButtonStyle-Zoom}" HorizontalAlignment="Left" Width="125"   ClickMode="Press"  Click="btn_Click" Height="35.088" Margin="4,2.335,0,0" VerticalAlignment="Top" FontWeight="Bold" Cursor="Hand" />
			<Ellipse x:Name="ep" Fill="{DynamicResource Linear_Silver}" Margin="0,10.997,11,14.578" Stroke="{DynamicResource Linear_BlackSilver}" Width="28.334" HorizontalAlignment="Right"/>
            <TextBlock x:Name="txt" TextWrapping="NoWrap" VerticalAlignment="Bottom" Margin="5,0,40,1.5" FontSize="10.667"/>
		</Grid>
	</Grid>
</UserControl>