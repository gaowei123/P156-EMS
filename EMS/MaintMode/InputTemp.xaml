<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="EMS.InputTemp"
	x:Name="UserControl"
	Width="180" Height="50">
	<Grid x:Name="LayoutRoot">
		<Grid>
			<Rectangle Stroke="{DynamicResource Linear_BlackSilver}" RadiusX="5" RadiusY="5" Fill="{DynamicResource ButtonNormalBackground}"/>
			<Ellipse x:Name="ep" Fill="{DynamicResource Linear_Silver}" Stroke="{DynamicResource Linear_BlackSilver}" Width="30" HorizontalAlignment="Right" Margin="0,8,3.906,8" Height="30"/>
			<TextBlock x:Name="txt_name" TextWrapping="NoWrap" Margin="6,2,44,0" FontSize="13.333" VerticalAlignment="Top" FontWeight="Bold" HorizontalAlignment="Center" Width="126.688"/>
			<TextBlock x:Name="txt_desc" TextWrapping="NoWrap" FontSize="12" Margin="4,23.597,37.906,0"/>
		</Grid>
	</Grid>
</UserControl>